<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_project_form" model="ir.ui.view">
            <field name="name">view.project.form.interface</field>
            <field name="model">res.project</field>
            <field name="inherit_id" ref="pabi_my_project.view_project_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="%(action_interface_update_project)d"
                            type="action"
                            string="Update Project"
                            class="oe_highlight"/>
                    <!-- <button string="Update Project" name="update_project" type="object"/> -->
                </xpath>
            </field>
        </record>
    </data>
</openerp>
