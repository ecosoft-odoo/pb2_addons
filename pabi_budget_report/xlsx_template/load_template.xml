<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- This function is called after all ir.attachment is loaded
            to load the file content to relevant template_ids -->
        <function model="ir.attachment" name="load_xlsx_template">
            <value eval="[
                ref('xlsx_report_budget_plan_unit_analysis'),
                ref('xlsx_report_budget_plan_project_analysis'),
                ref('xlsx_report_budget_plan_personnel_analysis'),
                ref('xlsx_report_asset_item_plan_analysis'),
                ref('xlsx_report_budget_plan_asset_analysis'),
                ref('xlsx_report_budget_summary'),
                ref('xlsx_report_budget_plan_construction_analysis'),
                ref('rpt_budget_commit_template'),
                ref('rpt_budget_actual_template'),
                ref('rpt_budget_future_commit_template'),
                ref('rpt_budget_future_commit_summary_template'),
            ]"/>
            <value>pabi_budget_report/xlsx_template</value>
        </function>
    </data>
</openerp>
