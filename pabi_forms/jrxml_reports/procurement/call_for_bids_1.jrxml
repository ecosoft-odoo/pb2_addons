pd.description as objective,
pd.standard_price,
pd.delivery_address as deliver_to,
pm.name as method,
ppr.name as price_range,
pd.doc_body as footer,
(SELECT name FROM purchase_order
WHERE requisition_id = pd.id ORDER BY name LIMIT 1) as  rfq_number,
(SELECT rp.name FROM res_partner rp
LEFT JOIN purchase_order po ON po.partner_id = rp.id
WHERE rp.id = po.partner_id AND po.requisition_id = pd.id ORDER BY po.name LIMIT 1) as rfq_supplier,
(SELECT rp.taxbranch FROM res_partner rp
LEFT JOIN purchase_order po ON po.partner_id = rp.id
WHERE rp.id = po.partner_id ORDER BY po.name LIMIT 1) as rfq_taxbranch,
(SELECT amount_total FROM purchase_order
WHERE requisition_id = pd.id ORDER BY name LIMIT 1) as rfq_amount_total,
(SELECT minimum_planned_date-DATE(now()) FROM purchase_order
WHERE requisition_id = pd.id ORDER BY name LIMIT 1) as minimum_planned_date,
pd.delivery_detail as delivery_detail,
pcon.name as purchase_condition,
