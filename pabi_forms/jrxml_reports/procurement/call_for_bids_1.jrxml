CONCAT(pcht.name,' ',pd.objective) as pd_objective,
(SELECT amount_total_text_th FROM purchase_order
WHERE requisition_id = pd.id ORDER BY name LIMIT 1) as amount_total_text_th,
cur.name as currency,
(select
ROUND(SUM(
case when atax.amount > 0.0 and atax.price_include = False then
	(((requestline.price_subtotal) +
