	<subDataset name="pr_detail">
		<parameter name="ids" class="java.lang.Integer">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<queryString>
			<![CDATA[SELECT
row_number() over (order by pdl.id) as pdl_count,
pdl.product_name,
pdl.price_unit * pd.currency_rate as price_unit,
(
select
case when atax.amount > 0.0 and atax.price_include = False then
	ROUND((reql.price_subtotal + sum(
	(atax.amount * reql.price_subtotal)
	) * pd.currency_rate)::DECIMAL,2)
else
	case when reql.price_subtotal < (reql.price_unit * reql.product_qty)
	then
		ROUND((reql.price_unit * reql.product_qty * pd.currency_rate)::DECIMAL, 2)
	else
		ROUND((reql.price_subtotal * pd.currency_rate)::DECIMAL, 2)
	end
end
FROM purchase_requisition req
LEFT JOIN purchase_requisition_line reql
ON req.id = reql.requisition_id
LEFT JOIN purchase_requisition_taxes_rel prtr
ON prtr.requisition_line_id = reql.id
LEFT JOIN account_tax atax
ON atax.id = prtr.tax_id
where reql.id = pdl.id
group by reql.price_subtotal,reql.price_unit,reql.product_qty, atax.id LIMIT 1) as amount_total,
(SELECT DISTINCT pr2.name FROM purchase_request_purchase_requisition_line_rel prpd
LEFT JOIN purchase_request_line prl
ON prl.id = prpd.purchase_request_line_id
LEFT JOIN purchase_requisition_line pdl2
ON pdl2.id = prpd.purchase_requisition_line_id
LEFT JOIN purchase_request pr2
ON pr2.id = prl.request_id
WHERE pdl2.id = pdl.id LIMIT 1) as material_group,
pdl.product_qty as product_qty,
(SELECT value FROM ir_translation it
WHERE it.res_id = pu.id AND it.name LIKE 'product.uom,name') as uom,
case when pdl.price_subtotal < (pdl.price_unit * pdl.product_qty)
then
(pdl.price_unit * pdl.product_qty) * pd.currency_rate
else
pdl.price_subtotal * pd.currency_rate
end
as price_subtotal,
cur.name as currency
FROM purchase_requisition_line pdl
LEFT JOIN purchase_requisition pd
ON pd.id = pdl.requisition_id
LEFT JOIN res_currency cur
ON cur.id = pd.currency_id
LEFT JOIN product_uom pu
ON pu.id = pdl.product_uom_id
where pdl.requisition_id = $P{ids}]]>
		</queryString>
